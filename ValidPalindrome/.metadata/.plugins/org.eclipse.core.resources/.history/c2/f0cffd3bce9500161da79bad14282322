
public class ValidPalindrome {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String s = "Red rum, sir, is murder";
		System.out.println(new ValidPalindrome().isAValidCharecter(';'));
	}
	
	
	/*public boolean isPalindrome(String s){
		
	}*/
	
	public boolean isAValidCharecter(char c){
		if(c >= 'a' && c <= 'z'){
			return true;
		}
		else if(c>='A' && c<='Z'){
			return true;
		}
		else if( (c-'0') >= 0 && (c - '0')<= 9){
			return true;
		}
		else return false;
	}
	
	
	
}
