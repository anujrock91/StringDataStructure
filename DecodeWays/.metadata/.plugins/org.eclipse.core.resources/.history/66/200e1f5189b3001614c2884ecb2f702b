import java.util.HashMap;

public class DecodeWays {
	
	private HashMap<Integer, Character> alphabetMap;
	
	public DecodeWays(){
		alphabetMap = new HashMap<>();
		alphabetMap.put(1, 'A');alphabetMap.put(10, 'J');alphabetMap.put('A', 1);
		alphabetMap.put(2, 'B');alphabetMap.put(11, 'K');alphabetMap.put('A', 1);
		alphabetMap.put(3, 'C');alphabetMap.put(12, 'L');alphabetMap.put('A', 1);
		alphabetMap.put(4, 'D');alphabetMap.put(13, 'M');alphabetMap.put('A', 1);
		alphabetMap.put(5, 'E');alphabetMap.put(14, 'N');alphabetMap.put('A', 1);
		alphabetMap.put(6, 'F');alphabetMap.put(15, 'O');alphabetMap.put('A', 1);
		alphabetMap.put(7, 'G');alphabetMap.put(16, 'P');alphabetMap.put('A', 1);
		alphabetMap.put(8, 'H');alphabetMap.put(17, 'Q');alphabetMap.put('A', 1);
		alphabetMap.put(9, 'I');alphabetMap.put(18, 'R');
	}
}
